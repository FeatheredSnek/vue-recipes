{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended",
        "plugin:nuxt/base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/multi-word-component-names": "off"
    }
}
